{
  "name": "Roaw",
  "version": "0.0.2",
  "manifest_version": 3,
  "description": "Ideias em desenvolvimento",
  "author": "Valdecir Merli",
  "options_ui": {
    "page": "Vweb/options.html",
    "open_in_tab": true
  },
  "action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "Vweb/newtab.html"
  },
  "permissions": [
    "background",
    "storage",
    "alarms",
    "declarativeContent",
    "activeTab",
    "scripting",
    "desktopCapture",
    "debugger",
    "downloads",
    "gcm",
    "geolocation",
    "identity",
    "idle",
    "management",
    "nativeMessaging",
    "notifications",
    "pageCapture",
    "power",
    "sessions",
    "system.cpu",
    "system.memory",
    "tabCapture",
    "tts",
    "ttsEngine",
    "webNavigation",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "*://*/*",
    "<all_urls>"
  ],
  "optional_permissions":[
    "activeTab",
    "scripting"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "*://*.vsmsystem.com/*",
      "*://vsmsystem.com/*",
      "*://localhost/*",
      "*://127.0.0.1/*"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "starting.js"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "finishing.js"
      ]
    }
  ],
  "devtools_page": "options.html",
  "web_accessible_resources": [
    {
      "resources":[
        "*.png",
        "imgs/*",
        "db/*",
        "injectable-scripts/*",
        "Vweb/vsm-bar.html",
        "Vweb/vsm-bar.js"
      ],
      "matches":["*://*/*"]
    }
  ]
}